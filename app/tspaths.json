{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/core/*": ["core/*"],
      "@/services/*": ["services/*"],
      "@/models/*": ["models/*"],
      "@/pages/*": ["pages/*"],
      "@/components/*": ["components/*"],
      "@/hooks/*": ["hooks/*"],
      "@/styles/*": ["styles/*"],
      "@/layouts": ["layouts"],
